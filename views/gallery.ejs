<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1  maximum-scale=1">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <meta name="HandheldFriendly" content="True">

    <link rel="icon" href="/img/yky-img/logo.png" type="image/x-icon">
    <link rel="stylesheet" href="/lib/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/materialize.min.css">
    <link rel="stylesheet" href="/css/normalize.css">
    <link rel="stylesheet" href="/css/app.css">
    <!-- materialize icon -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Owl carousel -->
    <link rel="stylesheet" href="/lib/owlcarousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="/lib/owlcarousel/assets/owl.theme.default.min.css">
    <!-- Magnific Popup core CSS /file -->
    <link rel="stylesheet" href="/lib/Magnific-Popup-master/dist/magnific-popup.css">

    <!-- PEAGE DEFAULT: SEO TAGS -->
    <meta name="author" content="Yay Kyi Yar || Trip Gallery">

    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name" content="Yay Kyi Yar || Trip Gallery">
    <meta itemprop="description"
        content="Myanmar’s first one-stop online travel portal, offering the largest inventory of hotels, flights, cars, buses, and tours, with multiple secure payment options for local and international travelers.">
    <meta itemprop="image" content="/img/yky-img/logo.png">

    <!-- Twitter Card data -->
    <meta name="twitter:card" content="Yay Kyi Yar || Trip Gallery">
    <meta name="twitter:site" content="Yay Kyi Yar Travel and Tours">
    <meta name="twitter:title"
        content="Visas, Flights, Hotels, Buses, Tours, Visa & Cars Booking - Myanmar | Yay Kyi Yar">
    <meta name="twitter:description"
        content="Myanmar’s first one-stop online travel portal, offering the largest inventory of hotels, flights, cars, buses, and tours, with multiple secure payment options for local and international travelers.">
    <meta name="twitter:image" content="/img/yky-img/logo.png">

    <!-- Open Graph data -->
    <meta property="og:title" content="Yay Kyi Yar || Trip Gallery" />
    <meta property="og:type" content="profile" />
    <meta property="og:url" content="http://yaykyiyar.com" />
    <meta property="og:image" content="/img/yky-img/logo.png" />
    <meta property="og:description"
        content="Myanmar’s first one-stop online travel portal, offering the largest inventory of hotels, flights, cars, buses, and tours, with multiple secure payment options for local and international travelers.">
</head>

<body id="homepage">
    <!-- BEGIN PRELOADING -->
    <div class="preloading">
        <div class="wrap-preload">
            <div class="cssload-loader"></div>
        </div>
    </div>
    <!-- END PRELOADING -->

    <!-- HEADER -->
    <header id="header">
        <div class="nav-wrapper container">
            <div class="header-logo">
                <a href="/" class="nav-logo"><i class="fa fa-plane" aria-hidden="true"></i>&nbsp;<span
                        class="lang chan-font" key="mn_yky"><%= navTitle %></span></a>
            </div>
            <div class="header-menu-button">
                <a href="/" data-activates="nav-mobile" class="button-collapse">
                    <div class="cst-btn-menu">
                        <i class="fa fa-bars" aria-hidden="true"></i>
                    </div>
                </a>
            </div>
        </div>
    </header>
    <!-- END HEADER -->

    <!-- SIDE NAV -->
    <nav>
        <ul id="nav-mobile" class="side-nav">
            <li class="profil">
                <img src="/img/logo.png" alt="profile">
                <h2><span class="lang chan-font" key="mn_yky"><%= navTitle %></span></h2>
                <h6><span class="lang chan-font" key="sn_travel_company"><%= company %></span></h6>
            </li>
            <li>
                <a href="/"><i class="fa fa-home" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_home">Home</span></a>
            </li>
            <li>
                <ul class="collapsible collapsible-accordion">
                    <li>
                        <div class="collapsible-header">
                            <i class="fa fa-globe" aria-hidden="true"></i>Change Language <span><i
                                    class="fa fa-caret-down"></i></span>
                        </div>
                        <div class="collapsible-body">
                            <ul>
                                <li>
                                    <a class="waves-effect waves-blue translate" id="en"><img src="./img/en.png" />
                                        English</a>
                                </li>
                                <li>
                                    <a class="waves-effect waves-blue translate" id="mm"><img src="./img/mm.png" />
                                        Myanmar</a>
                                </li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </li>
            <li>
                <a href="/destination"><i class="fa fa-map" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_desti">Destination</span></a>
            </li>
            <li>
                <a href="/team"><i class="fa fa-leaf" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_team">Our Team</span></a>
            </li>
            <li>
                <a href="/gallery"><i class="fa fa-camera" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_gallery">Gallery</span></a>
            </li>
            <li>
                <a href="/blog"><i class="fa fa-pencil-square" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_blog">Blog</span></a>
            </li>
            <li>
                <a href="/contact"><i class="fa fa-envelope" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_contact">Contact</span></a>
            </li>
            <li>
                <a href="/about"><i class="fa fa-info-circle" aria-hidden="true"></i><span class="lang chan-font"
                        key="sn_about">About us</span></a>
            </li>
        </ul>
    </nav>
    <!-- END SIDE NAV -->

    <!-- TOP MENU-->
    <div class="section top-header-menu">
        <div class="wrap-thm">
            <div class="container">
                <div class="row row-thm">
                    <div class="col s12">
                        <div class="thm-menu tooltipped" data-position="bottom" data-delay="50"
                            data-tooltip="This section still modifying...">
                            <i class="fa fa-user" aria-hidden="true"></i>&nbsp;<span class="lang chan-font"
                                key="tm_acc">Account</span>
                        </div>
                        <div class="thm-menu tooltipped" data-position="bottom" data-delay="50"
                            data-tooltip="This section still modifying...">

                            <i class="fa fa-globe" aria-hidden="true"></i>&nbsp;<span class="lang chan-font"
                                key="tm_trip">My Trip</span>
                        </div>
                        <div class="thm-menu tooltipped" data-position="bottom" data-delay="50"
                            data-tooltip="This section still modifying...">

                            <i class="fa fa-life-ring" aria-hidden="true"></i>&nbsp;<span class="lang chan-font"
                                key="tm_favo">favourite</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- END TOP MENU-->

    <!-- CONTENT -->
    <div id="page-content">
        <div class="section gallery">
            <div class="container">
                <div class="row">
                    <div class="col s12">
                        <div class="section-title">
                            <span class="theme-secondary-color">TRIP</span> GALLERY
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col s12">
                        <!-- filter -->
                        <div class="filter-gallery">
                            <button data-filter="gallery-no-filter" class="filter-button active">All</button>
                            <button data-filter="gallery-travel" class="filter-button">Travel</button>
                            <button data-filter="gallery-destination" class="filter-button">Destination</button>
                            <button data-filter="gallery-flight" class="filter-button">Flight</button>
                            <button data-filter="gallery-beach" class="filter-button">Beach</button>
                        </div>
                        <!-- /filter -->
                    </div>
                </div>
                <div class="row">
                    <div class="wrap-gallery">
                        <!-- All - Gallery -->
                        <div class="col s6 gallery-img-box gallery-destination">
                            <% jsonData.all.forEach( (all) => { %>
                            <a class="gallery-list" href="<%= all.img %>"><img alt="YKY-GALLERY"
                                    src="<%= all.img %>"></a>
                            <% }) %>
                        </div>
                        <!-- All - Gallery - Travel -->
                        <div class="col s6 gallery-img-box gallery-travel">
                            <% jsonData.travel.forEach( (travel) => { %>
                            <a class="gallery-list" href="<%= travel.img %>"><img alt="YKY-TRAVEL"
                                    src="<%= travel.img %>"></a>
                            <% }) %>
                        </div>
                        <!-- All - Gallery - flight -->
                        <div class="col s6 gallery-img-box gallery-flight">
                            <% jsonData.flight.forEach( (flight) => { %>
                            <a class="gallery-list" href="<%= flight.img %>"><img alt="YKY-FLIGHT"
                                    src="<%= flight.img %>"></a>
                            <% }) %>
                        </div>
                        <!-- All - Gallery - Beach -->
                        <div class="col s6 gallery-img-box gallery-beach">
                            <% jsonData.beach1.forEach( (beach1) => { %>
                            <a class="gallery-list" href="<%= beach1.img %>"><img alt="YKY-BEACH"
                                    src="<%= beach1.img %>"></a>
                            <% }) %>
                        </div>
                        <div class="col s6 gallery-img-box gallery-beach">
                            <% jsonData.beach2.forEach( (beach2) => { %>
                            <a class="gallery-list" href="<%= beach2.img %>"><img alt="YKY-BEACH"
                                    src="<%= beach2.img %>"></a>
                            <% }) %>
                        </div>
                        <!-- All - Gallery - Travel -->
                        <div class="col s6 gallery-img-box gallery-destination">
                            <% jsonData.gDestini.forEach( (destin) => { %>
                            <a class="gallery-list" href="<%= destin.img %>"><img alt="YKY-DESTINATION"
                                    src="<%= destin.img %>"></a>
                            <% }) %>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- END CONTENT -->

        <% include _layouts/footer %>